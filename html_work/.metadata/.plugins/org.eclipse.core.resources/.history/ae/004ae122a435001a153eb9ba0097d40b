package practice;

import java.util.*;

public class middleTest7 {

	public static void main(String[] args) {
		
		Scanner al = new Scanner(System.in);
		System.out.println("알파벳을 입력 하시오 : ");
		
		char eng = al.next().charAt0); 
		int parseInt = Integer.parseInt(eng+"");
		System.out.println(parseInt);

	}

}
