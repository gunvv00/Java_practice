package practice;

public class Practice01 {
	public static void main(String[] args) {
		
		int[] arr = new int[5];
		
		for(int i = 0; i<arr.length; i++) {
			arr [i] = i+1;
			System.out.println(arr[i]);
		}

		
		int[] tmp = arr;
		
		for(int i = 0; i<arr.length; i++) {
			tmp[i] = arr[i];
			arr = tmp;
			System.out.println(arr[i]);
			
		}
		
		
	}
}
