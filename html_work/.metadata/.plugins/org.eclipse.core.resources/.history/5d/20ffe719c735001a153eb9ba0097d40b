package practice;

public class Circle {
	public static void main(String[] args) {
		
		System.out.println("radius = 5");
		System.out.println("반지름이 5인 원의 넓이를 구하라");
		Circle cc = new Circle();
		double width = cc.CircleWidth(5,3.14);
		System.out.println(width);

	}

	static double CircleWidth(double radius, double pi) {
		double width = (radius * radius) * pi;
		
		return width;

	}
}
