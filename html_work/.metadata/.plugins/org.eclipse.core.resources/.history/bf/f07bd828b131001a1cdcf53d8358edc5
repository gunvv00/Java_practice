package practice;

public class ArrayEx9 {

	public static void main(String[] args) {
		int [] numArr = new int[10];
		int [] counter = new int[10];
		
		for(int num = 0; num < numArr.length; num++ ) {
			numArr[num] = (int)(Math.random() * 10 );
			System.out.print(numArr[num]);
			System.out.println("");
		}
	}

}
