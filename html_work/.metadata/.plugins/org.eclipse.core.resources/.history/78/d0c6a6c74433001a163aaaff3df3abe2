package practice;

public class middleTest4 {

	public static void main(String[] args) {
		int[][] dan = new int[8][9];

		for (int col = 1; col < 10; col++) {
			for (int row = 1; row < 9; row++) {
				int gob = col * row;
				dan[col][row] = gob;
				System.out.print(dan[col][row] + "\t");
			}System.out.println();
		}

	}

	
}
