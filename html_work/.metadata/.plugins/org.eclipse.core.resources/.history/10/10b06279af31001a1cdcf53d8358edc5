package st02.ch01;

public class VarEx5 {

	public static void main(String[] args) {
		
		boolean chk = true, chk2 = false;
		char ch = '_';
		int a = 10;
		int b = 210000000;
		double avg = 10 / 3.0;
		double d = 3.4;
		
		System.out.println("chk:" + chk + "chk2:" + chk2);
		System.out.println("ch:" + ch);
		System.out.println(a / 3);
		System.out.println(avg );
	}

}
