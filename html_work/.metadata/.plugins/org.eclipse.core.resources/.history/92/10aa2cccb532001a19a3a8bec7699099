package practice;

public class middleTest3 {

	public static void main(String[] args) {

		int[] arr = new int[5];

		for (int i = 0; i < arr.length; i++) {
			arr[i] = (int) (Math.random() * 100);
			System.out.print("임의의 수 출력 : "+ arr[i] + " ");
		}
		System.out.println();
		for(int i = 0 ; i < arr.length ; i++) {
			if(i%2 != 0) {
				System.out.print(arr[i] + " ");
			}
		}
	}

}
