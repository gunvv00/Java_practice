package practice;

public class middleTest8 {
	public static void main(String[] args) {
		
		middleTest8 mt = new middleTest8();
		int result = mt.test(5);
		System.out.print(result);
		
		
	}
	static int test(int n) {
		
		int result = 0;
		
		if(n ==  1) {
			
			return 1;
			
		}else {
			
			result = n * test(n-1);
			
		}

		
		return result;
	}

}
