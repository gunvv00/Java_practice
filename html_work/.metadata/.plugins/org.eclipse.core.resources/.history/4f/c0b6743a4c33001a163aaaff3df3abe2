package practice;

public class middleTest4 {

	public static void main(String[] args) {
		int[][] dan = new int[8][9];

		for (int col = 2; col < 10; col++) {
			for (int row = 1; row < 10; row++) {
				int hap = col * row; 
				System.out.print(hap + "\t");
			}
			System.out.println();
		}

	}

}
/*
 * 8 x 9 이차원 배열을 선언하세요 
 * 8 x 9 이차원 배열에 구구단의 결과값을 대입하세요
 */