package practice;

public class middleTest4 {

	public static void main(String[] args) {
		int[][] dan = new int[8][9];

		for (int row = 0; row < 8 ; row++) {
			for (int col = 0; row < 9; col++) {
				int hap = row * col; 
				System.out.print(hap + "\t");
			}
			System.out.println();
		}

	}

}
/*
 * 8 x 9 이차원 배열을 선언하세요 
 * 8 x 9 이차원 배열에 구구단의 결과값을 대입하세요
 */