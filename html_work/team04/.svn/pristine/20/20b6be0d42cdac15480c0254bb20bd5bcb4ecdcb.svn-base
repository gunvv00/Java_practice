package st02.ch01;

import java.util.*;

public class ForEx3 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.print("초기값을 입력하시오 : ");
		int sta = input.nextInt();
		System.out.print("끝값을 입력하시오 : " );
		int end = input.nextInt();
		int sum = 0;
		
		for(int i = sta; i <= end; i++) {
			sum += i;
		}
		System.out.print(sta + "부터" + end + "까지의 합은 : " + sum);

	}

}
